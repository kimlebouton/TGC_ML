        "id": "KYcxppRR5KWp"
      },
      "source": [
        "def goodbye(x,y):\n",
        "  for i in range(x):\n",
        "    print(\"hello\")\n",
        "  for i in range(y):\n",
        "    print(\"goodbye\")"
